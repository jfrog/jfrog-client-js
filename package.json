{
    "name": "xray-client-js",
    "version": "1.0.1",
    "description": "Xray Javascript Client is a javascript library, which wraps some of the REST APIs exposed by JFrog Xray.",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/jfrog/xray-client-js"
    },
    "bugs": {
        "url": "https://github.com/jfrog/xray-client-js/issues"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"model/**/*.ts\"",
        "build": "tsc --declaration",
        "lint": "tslint -p tsconfig.json",
        "prepare": "npm run build",
        "preversion": "npm run lint",
        "watch": "tsc -watch -p ./",
        "compile": "tsc -p ./",
        "pretest": "npm run compile",
        "test": "jest --verbose"
    },
    "files": [
        "dist/**/*"
    ],
    "dependencies": {
        "axios": "~0.19.2"
    },
    "devDependencies": {
        "@types/faker": "^4.1.11",
        "@types/http-proxy": "^1.17.4",
        "@types/jest": "^25.2.1",
        "faker": "^4.1.0",
        "http-proxy": "^1.18.0",
        "jest": "^25.5.0",
        "nock": "^12.0.3",
        "prettier": "^2.0.5",
        "ts-jest": "^25.4.0",
        "tslint": "^6.1.2",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^3.8.3"
    },
    "keywords": [
        "artifactory",
        "javascript",
        "typescript",
        "security",
        "nodejs",
        "devops",
        "jfrog",
        "xray",
        "scan",
        "js",
        "ts"
    ]
}
